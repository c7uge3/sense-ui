  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@umijs/fabric": "^2.5.7",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "docz": "^2.3.1",
    "husky": "^6.0.0",
